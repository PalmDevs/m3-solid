{
	"name": "m3-solid",
	"version": "0.1.0",
	"description": "Material Design 3 components for SolidJS",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"default": "./src/index.ts"
		},
		"./styles.css": {
			"default": "./src/styles.css"
		},
		"./themes/*.css": {
			"default": "./src/themes/*.css"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "vite --open",
		"build": "vite build && tsc --emitDeclarationOnly --outDir dist",
		"preview": "vite preview"
	},
	"dependencies": {
		"@iconify-icons/mdi": "^1.2.48",
		"@material/web": "^2.4.1",
		"@solid-primitives/refs": "^1.1.2",
		"solid-js": "^1.8.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@iconify/types": "^2.0.0",
		"@types/bun": "latest",
		"typescript": "^5.3.0",
		"vite": "^7.3.1",
		"vite-plugin-dts": "^4.5.4",
		"vite-plugin-solid": "^2.8.0"
	},
	"peerDependencies": {
		"solid-js": "^1.8.0"
	}
}
